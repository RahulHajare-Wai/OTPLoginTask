<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="OTPLoginTask.Views.Flyouts.FlyoutView" NavigationPage.HasNavigationBar="False"
             xmlns:pages="clr-namespace:OTPLoginTask.Views.Flyouts">
  <FlyoutPage.Flyout>
    <pages:FlyoutViewFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:FlyoutViewDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>